                </structure>
            </list-property>
            <xml-property name="queryText"><![CDATA[SELECT * FROM
(
SELECT SUM(i2.current_quantity)as quan,i2.drug_id,i1.store_id,id.`name`,idf.dozage,idc.`name`  as category ,idf.`name` as formulation,i2.formulation_id 
FROM `inventory_store_drug_transaction` as i1
INNER JOIN inventory_store_drug_transaction_detail as i2 ON i1.id = i2.transaction_id
inner join inventory_drug id on id.id=i2.drug_id
inner join inventory_drug_category idc on idc.id=id.category_id
inner join inventory_drug_formulation idf on  i2.formulation_id=idf.id
where i1.store_id = ? and i2.date_expiry>date(NOW())
GROUP BY i2.drug_id,i2.formulation_id
order by id.`name`
)as inq WHERE inq.quan>0;]]></xml-property>
