            <xml-property name="queryText"><![CDATA[select *
from 
(SELECT SUM(i2.current_quantity) AS quan,iisc.`name` AS subcat,ii.`name` AS item,iic.`name`,iic.id AS cid
FROM `inventory_store_item_transaction` AS i1
INNER JOIN inventory_store_item_transaction_detail AS i2 ON i1.id = i2.transaction_id
INNER JOIN inventory_item AS ii ON i2.item_id=ii.id
INNER JOIN inventory_item_sub_category iisc ON ii.sub_category_id=iisc.id
INNER JOIN inventory_item_category iic on ii.category_id=iic.id
WHERE i1.store_id = ? AND iic.id=?
GROUP BY i2.item_id
ORDER BY ii.`name`
) AS itmq WHERE itmq.quan>0;]]></xml-property>
