            <xml-property name="queryText"><![CDATA[SELECT i.drug_id,i.formulation_id,SUM(i.issue_quantity),i.issue_quantity,idp.store_id,idp.patient_id,idp.identifier,i.created_on,idp.created_on,idf.name AS 'formulation',idf.dozage,idr.name'drug',pa.value,p.birthdate,CONCAT_WS(' ',pn.given_name,pn.middle_name,pn.family_name)
FROM inventory_store_drug_transaction_detail i
INNER JOIN inventory_store_drug_patient_detail isd ON i.id=isd.transaction_detail_id
INNER JOIN inventory_store_drug_patient idp ON isd.store_drug_patient_id=idp.id
INNER JOIN inventory_drug_formulation idf ON i.formulation_id=idf.id
INNER JOIN inventory_drug idr ON i.drug_id=idr.id
INNER JOIN person_attribute pa ON idp.patient_id=pa.person_id AND pa.person_attribute_type_id=(SELECT p.`person_attribute_type_id` FROM person_attribute_type p WHERE p.name='Patient category')
INNER JOIN person p ON idp.patient_id=p.person_id
INNER JOIN person_name pn ON p.person_id=pn.person_id
INNER JOIN inventory_store_drug_transaction it ON i.transaction_id=it.id
INNER JOIN inventory_store ist ON it.store_id=ist.id AND ist.name LIKE '%pharmacy%'
WHERE  DATE(i.created_on) BETWEEN ? AND ?
GROUP BY i.drug_id,idp.identifier,idf.id]]></xml-property>
